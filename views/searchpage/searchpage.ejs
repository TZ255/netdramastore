<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../Global/google.ejs'); %>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="robots" content="noindex,nofollow,noarchive" />
    <title>List of All Dramas</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.0/css/all.min.css"
        integrity="sha512-10/jx2EXwxxWqCLX/hHth/vu2KY3jCF70dCQB8TSgNjbCVAC/8vai53GfMDrO2Emgwccf2pJqxct9ehpzG+MTw=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />

    <%- include('./partials/style.ejs'); %>
    <%- include('../Global/nav-style.ejs'); %>
    <%- include('../Global/ads/style.ejs'); %>
</head>

<body>
    <%- include('../Global/navhtml.ejs'); %>

    <div class="searchBoxCont">
        <input type="text" id="searchBox" oninput="filtering()" placeholder="🔍 search drama here . . .">
        <div class="allDrama">
            <ul id="ulAllDrama">
                <% allDrama.forEach(drama=> { %>
                <li class="liDrama" onclick="window.open('<%= drama.path %>', '_self')">
                    <%= drama.name %>
                </li>
                <% }) %>
                <li id="notf" onclick="window.open('tg://resolve?domain=shemdoe', '_self')">Not find it...
                    request here</li>
            </ul>
        </div>
    </div>
<br><br>
    <div id="d2024" class="most d2024">
        <div class="header">
            <span class="title">All Dramas</span>
        </div>
    </div>
    <div class="alls">
        <ol class="list-group list-group-numbered">
            <% allDrama.forEach(drama=> { %>
            <li onclick="window.open('<%= drama.path %>', '_self')" class="list-group-item">
                <%= drama.name %>
            </li>
            <% }) %>
        </ol>
    </div>

    <footer>
        <span>
            Korean Drama Store
        </span>
        <span class="a" onclick="window.open('tg://join?invite=mTx_t-6TBx9hNTc8', '_self')">
            <i class="fa-brands fa-telegram"></i> Join Us Telegram
        </span>
        <span id="dated">
        </span>
    </footer>


    <%- include('./partials/script.ejs'); %>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>

    <%- include('../Global/ads/vignette.ejs'); %>
</body>

</html>